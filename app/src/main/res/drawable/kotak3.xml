<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:left="50dp">
        <shape android:shape="rectangle">
            <corners android:bottomRightRadius="8dp" android:topRightRadius="8dp"/>
            <size android:height="100dp" android:width="5dp"/>
            <solid android:color="@color/green600"/>
        </shape>
    </item>
    <item android:right="100dp">
        <shape android:shape="rectangle">
            <corners android:bottomLeftRadius="8dp" android:topLeftRadius="8dp"/>
            <size android:height="100dp" android:width="150dp"/>
            <solid android:color="@color/green800"/>
        </shape>
    </item>


</layer-list>